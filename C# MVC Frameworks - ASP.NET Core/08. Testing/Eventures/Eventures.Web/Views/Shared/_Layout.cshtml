<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<title>@ViewData["Title"] - Eventures</title>
	<link rel="stylesheet" href="/lib/bootstrap/dist/css/bootstrap.css" />
	<link rel="stylesheet" href="/css/reset-css.css">
	<link rel="stylesheet" href="/css/bootstrap.min.css">
	<link rel="stylesheet" href="/css/site.css">
</head>
<body>
	<header>
		<nav class="navbar navbar-expand-sm eventures-bg-color">
			<a class="navbar-brand nav-link-dark" asp-controller="Home" asp-action="Index">Eventures</a>
			<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarToggle" aria-controls="navbarToggle"
					aria-expanded="false" aria-label="Toggle navigation">
				<span class="navbar-toggler-icon"></span>
			</button>
			<div class="collapse navbar-collapse" id="navbarToggle">
				<partial name="_NavbarPartial" />
			</div>
		</nav>
	</header>
	<div class="container-fluid">
		<main>
			@RenderBody()
		</main>
		<footer>
			<div class="eventures-bg-color">
				<div class="p-2 text-center">
					&copy; CopyRight Eventures Inc. 2018-2018. All Rights Reserved.
				</div>
			</div>
		</footer>
	</div>
	<script src="/lib/jquery/dist/jquery.js"></script>
	<script src="/lib/bootstrap/dist/js/bootstrap.js"></script>
	<script src="/lib/jquery-validation/dist/jquery.validate.js"></script>
	<script src="/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
</body>
</html>